<!doctype html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <title>#(title)</title>
        <link rel="stylesheet" href="/styles.css">

        <!-- OpenGraph Stuff -->
        <meta property="og:site_name" content="SwiftTok"/>
        <meta property="og:type" content="website"/>
        <meta property="og:title" content="#(user) on TikTok"/>
        <meta property="og:description" content="#(user)'s short video with â™¬ #(songName)"/>
        <meta property="og:url" content="https://vm.tiktok.com/#(id)"/>
    </head>
    <body>
        <h1>#(title)</h1>
        <video controls class="fullscreen" autoplay>
            <source src="#(src)" type="video/mp4">
        </video>
    </body>
</html>
